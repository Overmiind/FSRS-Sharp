<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    
    <PropertyGroup Label="deploy">
        <Version>1.0.0</Version>
        <RepositoryUrl>https://github.com/Overmiind/FSRS-Sharp</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>flashcard spaced-repetition open-spaced-repetition fsrs srs anki spaced-repetition-algorithm osr dotnet c#</PackageTags>
        <PackageProjectUrl>https://github.com/Overmiind/FSRS-Sharp</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <Title>PuppeteerExtraSharp</Title>
        <Authors>Overmind</Authors>
    </PropertyGroup>
</Project>
